extends ../base.pug

block variables
  - var template = 'about'

block content
  .about
    .about__wrapper
      section.about__gallery
        .about__gallery__wrapper
          each media in about.data.about_gallery
            figure.about__gallery__media
              img.about__gallery__media__image(alt=media.about_image.alt src=media.about_image.url)

